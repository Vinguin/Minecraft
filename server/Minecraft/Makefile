# Dieses Makefile ist fÃ¼r Minecraft gedacht.

docker_image:
	docker build -t minecraft-server . 

docker_run:
	docker run -d -it -v /root/Minecraft:/home/Minecraft -p 25001:25001 --name minecraft-server minecraft-server

docker_stop:
	docker stop minecraft-server

docker_start:
	docker start minecraft-server
